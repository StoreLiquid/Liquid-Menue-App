{
  "framework": "nextjs",
  "installCommand": "npm install && npm install --save-dev @types/react@18.2.38 @types/react-dom@18.2.15 @types/node@20.10.0",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "headers": [
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 